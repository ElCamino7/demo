pipeline{
  agent any

  parameters{
    choice(name: "Version", choices: ["0.0.1","0.0.2"])
  }
  stages{
    
    stage("Salutami"){
      steps{
      echo "Hello World"
      }
    }

    stage("Joe Pesci"){
      steps{
        echo "Buffo come"
      }
    }
    stage("De Niro"){
      steps{
        echo "Buffo come"
      }
    }
    
  }
}
  
