pipeline{
  agent any
  parameters {
        gitParameter name: "TAG",
                     type: "PT_TAG",
                     defaultValue: "0.0.1",
                     listSize: '10'
    }
  stages{
    
    stage("Salutami"){
      steps{
      echo "Hello World"
      }
    }

    stage("Joe Pesci"){
      
      steps{
        echo "Buffo come"
      }
    }
    
    stage("De Niro"){
      steps{
        echo "Buffo come"
     
      }
    }
    
  }
}
  
